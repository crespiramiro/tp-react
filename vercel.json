{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "zeroConfig": true },
        "build": {
          "publish": "/",
          "command": "npm run build && npm run json"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/products",
        "dest": "http://localhost:5050"
      }
    ]
  }
  